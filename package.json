{
  "name": "notesaw_preview",
  "description": "none",
  "version": "0.0.1",
  "publisher": "Appleblue17",
  "engines": {
    "vscode": "^1.100.0"
  },
  "type": "module",
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "notesaw",
        "extensions": [
          ".note"
        ],
        "aliases": [
          "Notesaw",
          "note"
        ]
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "notesaw.showPreview",
          "when": "editorLangId == notesaw",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {
        "command": "notesaw.showPreview",
        "title": "Start Notesaw preview to the Side",
        "category": "Notesaw",
        "icon": "$(open-preview)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "process": "tsc -p ./ && node ./out/main.js"
  },
  "dependencies": {
    "feather-icons": "^4.29.2",
    "rehype-document": "^7.0.3",
    "rehype-format": "^5.0.1",
    "rehype-katex": "^7.0.1",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "~20.17.58",
    "@types/unist": "^3.0.3",
    "@types/vscode": "^1.103.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2"
  }
}
